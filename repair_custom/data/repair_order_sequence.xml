<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_repair_order" model="ir.sequence">
            <field name="name">Ordre de Réparation</field>
            <field name="code">repair.order</field>
            <field name="prefix">RO/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>
        <record id="seq_repair_batch" model="ir.sequence">
            <field name="name">Séquence Dossier Réparation</field>
            <field name="code">repair.batch</field> 
            <field name="padding">3</field> 
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>